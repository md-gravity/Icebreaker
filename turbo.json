{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["^build", "prisma:deploy"]
    },
    "debug": {
      "cache": false,
      "dependsOn": ["^build", "prisma:deploy"]
    },
    "watch": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "build": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"],
      "outputs": ["build/**", ".next/**"],
      "dependsOn": ["^build", "prisma:generate"]
    },
    "prisma:generate": {
      "cache": false
    },
    "prisma:deploy": {
      "cache": false
    },
    "typecheck": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    }
  }
}