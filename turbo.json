{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "setup": {
      "cache": false,
      "dependsOn": ["^setup"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "watch": {
      "cache": false
    },
    "build": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts"],
      "outputs": ["build/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": []
    },
    "lint:fix": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": []
    }
  }
}