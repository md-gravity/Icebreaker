// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_HOST")
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  email    String  @unique
  temporal Boolean @default(false)
}

model Room {
  id   Int     @id @default(autoincrement())
  url  String  @unique
  name String?
}
