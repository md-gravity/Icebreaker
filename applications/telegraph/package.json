{
  "name": "@applications/telegraph",
  "version": "0.0.1",
  "main": "./build/index.js",
  "scripts": {
    "start": "tsx src/main.ts",
    "dev": "tsx watch --clear-screen=false src/main.ts",
    "watch": "tsc --watch",
    "build": "tsc --build --clean && tsc && tsc-alias",
    "===utils===": "===utils===",
    "postinstall": "npm run prisma:generate && npm run build",
    "typecheck": "tsc --noEmit true",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "===prisma===": "===prisma===",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --create-only --name",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@applications/passport": "^0.0.1",
    "@packages/authentication": "0.0.1",
    "@packages/duct": "0.0.1",
    "@prisma/client": "^4.10.1",
    "@trpc/server": "^10.20.0",
    "prisma": "^4.10.1",
    "ws": "^8.12.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@packages/eslint-config-hub": "1.0.0",
    "@packages/ts-configs": "1.0.0",
    "@types/node": "^18.11.3",
    "@types/ws": "^8.5.4",
    "tsc-alias": "^1.8.2"
  },
  "eslintConfig": {
    "extends": [
      "@packages/eslint-config-hub/.base.js"
    ]
  },
  "prettier": "@packages/eslint-config-hub/.prettierrc.js"
}
