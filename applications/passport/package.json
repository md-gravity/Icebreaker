{
  "name": "@applications/passport",
  "version": "0.0.1",
  "main": "./build/index.js",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/main.ts",
    "debug": "tsx watch --inspect=0.0.0.0:9229 --clear-screen=false src/main.ts",
    "start": "tsx src/main.ts",
    "watch": "tsc --watch",
    "build": "tsc --build --clean && tsc && tsc-alias",
    "===utils===": "===utils===",
    "typecheck": "tsc --noEmit true",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "===prisma===": "===prisma===",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --create-only --name",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "===scripts===": "===scripts===",
    "postinstall": "npm run build",
    "prepare": "npm run prisma:generate"
  },
  "dependencies": {
    "@packages/authentication": "*",
    "@packages/duct": "*",
    "@packages/dtos": "*",
    "@prisma/client": "^4.10.1",
    "@trpc/server": "^10.20.0",
    "prisma": "^4.10.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@packages/eslint-config-hub": "*",
    "@packages/ts-configs": "*",
    "@types/node": "^18.11.3",
    "tsc-alias": "^1.8.2"
  },
  "eslintConfig": {
    "extends": [
      "@packages/eslint-config-hub/.base.js"
    ]
  },
  "prettier": "@packages/eslint-config-hub/.prettierrc.js"
}
