{
  "name": "@gravity/icebreaker",
  "version": "0.0.1",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "workspaces": [
    "applications/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "===dev===": "===dev===",
    "dev:passport": "turbo dev --filter=passport",
    "dev:web": "turbo dev --filter=web"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "tsx": "^3.12.6",
    "turbo": "^1.6.3",
    "typescript": "^5.0.3"
  }
}
